{
	"name": "flutter-dependency-docs",
	"version": "1.0.8",
	"publisher": "djbkwon",
	"displayName": "Flutter Links",
	"description": "Show pub.dev links for your Flutter dependencies.",
	"engines": {
		"vscode": "^1.32.0"
	},
	"license": "MIT",
	"categories": [
		"Other"
	],
	"icon": "img/icon.png",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.8.3",
		"vscode": "^1.1.37"
	},
	"activationEvents": [
		"onLanguage:yaml",
		"onCommand:extension.viewDependency"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "view-dependency",
			"properties": {
				"search.baseUrl": {
					"type": "string",
					"default": "https://pub.dev/",
					"description": "Show pub.dev links for your flutter dependencies."
				}
			}
		},
		"commands": [
			{
				"command": "extension.viewDependency",
				"title": "Show pub.dev links for your flutter dependencies."
			}
		]
	},
	"homepage": "https://github.com/danieljbk/flutter-links",
	"repository": {
		"type": "git",
		"url": "https://github.com/danieljbk/flutter-links.git"
	},
	"bugs": {
		"url": "https://github.com/danieljbk/flutter-links/issues",
		"email": "djbkwon@gmail.com"
	}
}
